{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "client",
  "projects": {
    "admin": {
      "projectType": "application",
      "schematics": {},
      "root": "client/admin",
      "sourceRoot": "client/admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/admin",
            "index": "client/admin/src/index.html",
            "main": "client/admin/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "client/admin/tsconfig.app.json",
            "assets": [
              "client/admin/src/favicon.ico",
              "client/admin/src/assets"
            ],
            "styles": [
              "client/admin/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1.5mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "optimization": true,
              "aot": true,
              "namedChunks": false,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "admin:build:production"
            },
            "development": {
              "browserTarget": "admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "client/admin/tsconfig.spec.json",
            "assets": [
              "client/admin/src/favicon.ico",
              "client/admin/src/assets"
            ],
            "styles": [
              "client/admin/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "shop": {
      "projectType": "application",
      "schematics": {},
      "root": "client/shop",
      "sourceRoot": "client/shop/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shop",
            "index": "client/shop/src/index.html",
            "main": "client/shop/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "client/shop/tsconfig.app.json",
            "assets": [
              "client/shop/src/favicon.ico",
              "client/shop/src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules\\bootstrap\\dist\\css\\bootstrap.min.css",
              "client/shop/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1.5mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "optimization": true,
              "aot": true,
              "namedChunks": false,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shop:build:production"
            },
            "development": {
              "browserTarget": "shop:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "client/shop/tsconfig.spec.json",
            "assets": [
              "client/shop/src/favicon.ico",
              "client/shop/src/assets"
            ],
            "styles": [
              "client/shop/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "2c34837b-e5bc-4f33-9cfa-6d8355e83f71"
  }
}
